{
  "name": "emr-bridge",
  "version": "1.6.0",
  "description": "Allows you to easily give access to main methods from the renderer process",
  "main": "esm/index.js",
  "scripts": {
    "compile": "tsc --project ./tsconfig.json && tsc --project ./tsconfig.cjs.json"
  },
  "types": "esm/index.d.ts",
  "files": [
    "esm",
    "cjs"
  ],
  "typesVersions": {
    "*": {
      ".": [
        "./esm/index.d.ts"
      ],
      "exp": [
        "./esm/exp.d.ts"
      ],
      "cjs": [
        "./cjs/index.d.ts"
      ],
      "cjs/exp": [
        "./cjs/exp.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./esm/index.js",
    "./exp": "./esm/exp.js",
    "./package.json": "./package.json",
    "./cjs": "./cjs/index.js",
    "./cjs/exp": "./cjs/exp.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VerZsuT/emr-bridge.git"
  },
  "keywords": [
    "electron",
    "ipc"
  ],
  "author": "VerZsuT",
  "license": "MIT",
  "peerDependencies": {
    "electron": ">=20.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
